{
  "name": "universe",
  "productName": "Universe",
  "description": "Universe app",
  "version": "0.0.44",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TomTomB/universe.git"
  },
  "author": {
    "name": "Tom-Rune Bornholdt",
    "email": "kontakt@bornholdt.digital",
    "url": "https://bornholdt.digital"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TomTomB/universe/issues"
  },
  "homepage": "https://github.com/TomTomB/universe#readme",
  "scripts": {
    "commit": "npx git-cz",
    "build": "yarn build:version && concurrently \"yarn build:main\" \"yarn build:renderer\"",
    "build:main": "cross-env NODE_ENV=production webpack --config ./config/webpack.config.main.prod.babel.js",
    "build:renderer": "cross-env NODE_ENV=production webpack --config ./config/webpack.config.renderer.prod.babel.js",
    "build:dll": "cross-env NODE_ENV=development webpack --config ./config/webpack.config.renderer.dev.dll.babel.js",
    "build:version": "node -r @babel/register ./scripts/GenerateVersionFile.js",
    "rebuild": "electron-rebuild --parallel --types prod,dev,optional --module-dir src/main",
    "lint": "cross-env NODE_ENV=development eslint . --cache --ext .js,.jsx,.ts,.tsx",
    "package": "yarn build && electron-builder build --publish never",
    "package:publish": "yarn build && electron-builder build --publish always --win --mac --linux",
    "postinstall": "yarn --cwd src/main install && husky install && yarn build:version && yarn build:main && node -r @babel/register ./scripts/CheckNativeDep.js && electron-builder install-app-deps && yarn cross-env NODE_ENV=development webpack --config ./config/webpack.config.renderer.dev.dll.babel.js && yarn-deduplicate yarn.lock",
    "start": "yarn build:version && node -r @babel/register ./scripts/CheckPortInUse.js && cross-env yarn start:renderer",
    "start:main": "cross-env NODE_ENV=development electron -r ./scripts/BabelRegister ./src/main/main.ts",
    "start:renderer": "cross-env NODE_ENV=development webpack serve --config ./config/webpack.config.renderer.dev.babel.js",
    "test": "yarn build:version && jest",
    "release": "standard-version && git push --follow-tags origin main",
    "fix:upstream": "rm .git/refs/remotes/origin/main && git fetch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "@babel/plugin-proposal-do-expressions": "^7.12.13",
    "@babel/plugin-proposal-export-default-from": "^7.12.13",
    "@babel/plugin-proposal-export-namespace-from": "^7.12.13",
    "@babel/plugin-proposal-function-bind": "^7.12.13",
    "@babel/plugin-proposal-function-sent": "^7.12.13",
    "@babel/plugin-proposal-json-strings": "^7.13.8",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.13.8",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.13.8",
    "@babel/plugin-proposal-optional-chaining": "^7.13.8",
    "@babel/plugin-proposal-pipeline-operator": "^7.12.13",
    "@babel/plugin-proposal-throw-expressions": "^7.12.13",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-react-constant-elements": "^7.13.10",
    "@babel/plugin-transform-react-inline-elements": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/register": "^7.13.8",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@storybook/addon-actions": "6.2.2",
    "@storybook/addon-essentials": "6.2.2",
    "@storybook/addon-links": "6.2.2",
    "@storybook/react": "6.2.2",
    "@teamsupercell/typings-for-css-modules-loader": "^2.4.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@types/classnames": "^2.2.11",
    "@types/enzyme": "^3.10.8",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.21",
    "@types/node": "14.14.35",
    "@types/overlayscrollbars": "^1.12.0",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-test-renderer": "^17.0.1",
    "@types/shortid": "^0.0.29",
    "@types/styled-components": "^5.1.9",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-dev-expression": "^0.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-styled-components": "^1.12.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "chalk": "^4.1.0",
    "concurrently": "^6.0.0",
    "copy-webpack-plugin": "^8.0.0",
    "core-js": "^3.9.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.1.3",
    "css-minimizer-webpack-plugin": "^1.3.0",
    "detect-port": "^1.3.0",
    "electron": "^12.0.1",
    "electron-builder": "^22.10.5",
    "electron-devtools-installer": "^3.1.1",
    "electron-notarize": "^1.0.0",
    "electron-rebuild": "^2.3.5",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "enzyme-to-json": "^3.6.1",
    "eslint": "^7.22.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-flowtype": "^5.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^3.10.2",
    "eslint-webpack-plugin": "^2.5.2",
    "git-describe": "^4.0.4",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^5.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "mini-css-extract-plugin": "^1.3.9",
    "node-sass": "^5.0.0",
    "prettier": "^2.2.1",
    "react-refresh": "^0.9.0",
    "react-test-renderer": "^17.0.1",
    "rimraf": "^3.0.2",
    "sass-loader": "^11.0.1",
    "standard-version": "^9.1.1",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "typescript": "^4.2.3",
    "webpack": "^5.27.1",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-glsl-loader": "^1.0.1",
    "webpack-merge": "^5.7.3",
    "yarn-deduplicate": "^3.1.0"
  },
  "dependencies": {
    "@imjs/electron-differential-updater": "^5.1.4",
    "@reduxjs/toolkit": "^1.5.0",
    "@sentry/electron": "^2.4.0",
    "@xstate/react": "^1.3.1",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.9.1",
    "electron-log": "^4.3.2",
    "electron-updater": "^4.3.8",
    "gl-matrix": "^3.3.0",
    "history": "^4.10.1",
    "overlayscrollbars": "^1.13.1",
    "overlayscrollbars-react": "^0.2.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^6.15.5",
    "react-popper-tooltip": "^4.2.0",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-spring": "^8.0.27",
    "regenerator-runtime": "^0.13.7",
    "shortid": "^2.2.16",
    "styled-components": "^5.2.1",
    "xstate": "^4.17.0",
    "yup": "^0.32.9"
  },
  "devEngines": {
    "node": ">=10.x",
    "npm": ">=6.x",
    "yarn": ">=1.21.3"
  },
  "browserslist": [
    "last 1 electron version"
  ],
  "renovate": {
    "extends": [
      "bliss"
    ],
    "baseBranches": [
      "next"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "cross-env NODE_ENV=development eslint --cache"
    ],
    "{*.json,.{babelrc,eslintrc,prettierrc}}": [
      "prettier --ignore-path .eslintignore --parser json --write"
    ],
    "*.{css,scss}": [
      "prettier --ignore-path .eslintignore --single-quote --write"
    ],
    "*.{html,md,yml}": [
      "prettier --ignore-path .eslintignore --single-quote --write"
    ]
  },
  "build": {
    "productName": "Universe",
    "appId": "digital.bornholdt.Universe",
    "files": [
      "package.json",
      "node_modules/",
      "main.js",
      "dist/",
      "splash/"
    ],
    "afterSign": "scripts/Notarize.js",
    "mac": {
      "target": [
        "dmg"
      ],
      "type": "distribution",
      "hardenedRuntime": true,
      "entitlements": "assets/entitlements.mac.plist",
      "entitlementsInherit": "assets/entitlements.mac.plist",
      "gatekeeperAssess": false
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Development"
    },
    "directories": {
      "app": "intermediate",
      "buildResources": "assets",
      "output": "release"
    },
    "extraResources": [
      "./assets/**"
    ],
    "publish": {
      "provider": "github",
      "owner": "TomTomB",
      "repo": "universe",
      "releaseType": "release"
    }
  },
  "jest": {
    "testURL": "http://localhost/",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|ogg)$": "<rootDir>/mocks/fileMock.js",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json"
    ],
    "moduleDirectories": [
      "node_modules",
      "src/main/node_modules"
    ],
    "setupFiles": [
      "./scripts/SetupJest.js",
      "./scripts/CheckBuildsExist.js"
    ]
  }
}
